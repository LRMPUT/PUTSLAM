<?xml version="1.0" encoding="UTF-8"?>
  <!-- Visualizer configuration -->
  <VisualizerConfig>
  	<name>QGLVisualizer</name>
	<background red="0.1" green="0.1" blue="0.1" alpha="1.0"></background>
	<trajectory drawTrajectory="true" red="1.0" green="0.0" blue="0.0" alpha="1.0" width="2.5"></trajectory>
	<trajectoryPoints drawTrajectoryPoints="true" red="0.0" green="0.0" blue="0.3" alpha="1.0" size="0.001" accuracy="20"></trajectoryPoints>
	<features drawFeatures="true" red="0.0" green="0.8" blue="0.0" alpha="1.0" size="0.015" smoothness="20"></features>
	<pose2feature drawLinks="false" red="0.0" green="0.0" blue="0.8" alpha="0.1" width="0.5"></pose2feature>
	<pointCloud drawPointClouds="true" cloudPointSize="5"></pointCloud>
	<camera flyingCamera="false"></camera>
  </VisualizerConfig>