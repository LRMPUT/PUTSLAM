<?xml version="1.0" encoding="UTF-8"?>
<!-- File Model-->
<!--	
	verbose :	0 - no debugging information
			1 - print read parameters
			2 - print read parameters & getchar

	Supported files:
		TUM Freiburg dataset:
		- freiburg1_360.xml
		- freiburg1_desk.xml
		- freiburg1_desk2.xml
		- freiburg1_room.xml
		- freiburg2_desk.xml
		- freiburg3_long_office_household.xml
		ICL NUIM dataset:
		- icl_office_room_traj1_loop.xml
		- icl_office_room_traj2_loop.xml
		- icl_office_room_traj2_loop_noise.xml
		- icl_office_room_traj3_loop.xml
		PUT Kin1vs2 dataset:
		- kin1vs2_dataset2_kinect1.xml
		- kin1vs2_dataset2_kinect2.xml
		- ...
		- kin1vs2_dataset9_kinect1.xml
		- kin1vs2_dataset9_kinect2.xml
		Messor datasets;
		- messor_3.xml
-->
<Model verbose="0" datasetFile="datasetConfig/icl_office_room_traj2_loop.xml"/>

<!-- Parameters defining the way the data is played from files:
  	verbose :	0 - no debugging information
			1 - print read parameters
			2 - print read parameters & getchar

	playEveryNthFrame 	-> only every Nth frame is used in motion estimation
  	realTime 			-> overrides 'playEveryNthFrame' and simulates real-time operation by skipping frames
 	maxNumberOfFrames	-> stop processing after maxNumberOfFrames, -1 to turn off -->
<playParameters verbose="0" playEveryNthFrame="1" realTime="0" maxNumberOfFrames="4000"/>

<pose x="0.0" y="0.0" z="0.0" qw="1" qx="0" qy="0" qz="0"/>
